                <button class="carousel-control-next" type="button" data-bs-target="#carouselMotiSpectra" data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                </button>
